{
    "name": "xentixar/workflow-manager",
    "type": "library",
    "version": "2.0.1",
    "license": "MIT",
    "description": "A workflow manager plugin for FilamentPHP with PHP enum support.",
    "authors": [
        {
            "name": "xentixar",
            "email": "xentixar@gmail.com",
            "role": "Software Engineer"
        }
    ],
    "keywords": [
        "laravel",
        "filament",
        "workflow",
        "manager",
        "workflow-manager",
        "workflow-manager-plugin",
        "plugin",
        "enum",
        "state-machine"
    ],
    "require": {
        "filament/support": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Xentixar\\WorkflowManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xentixar\\WorkflowManager\\WorkflowManagerServiceProvider"
            ]
        }
    },
    "options":{
        "symlink" : true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
